teTime === undefined) {\n        time = new Date(this.item.start.date)\n      } else {\n        time = new Date(this.item.start.dateTime)\n      }\n      var now = new Date(Date.now())\n      var diff = Math.round((time - now) / (1000 * 60 * 60 * 24))\n      if (diff === 0) return 'Today'\n      if (diff === 1) return 'Tomorrow'\n      if (diff < 10 && diff > -1) {\n        return (\n          Intl.DateTimeFormat('en-US', { weekday: 'long' }).format(time) +\n          ' (In ' +\n          diff +\n          ' days)'\n        )\n      } else if (now.getMonth() === time.getMonth()) {\n        return (\n          Intl.DateTimeFormat('en-US', { weekday: 'long' }).format(time) +\n          ' ' +\n          time.getDate() +\n          '.'\n        )\n      } else if (now.getFullYear() === time.getFullYear()) {\n        return (\n          Intl.DateTimeFormat('en-US', { weekday: 'long' }).format(time) +\n          ' ' +\n          time.getDate() +\n          '. ' +\n          Intl.DateTimeFormat('en-US', { month: 'long' }).format(time)\n        )\n      }\n      return (\n        time.getDate() +\n        '. ' +\n        Intl.DateTimeFormat('en-US', { month: 'long' }).format(time) +\n        ' ' +\n        time.getFullYear()\n      )\n    },\n    getTime: function () {\n      if (this.item.start.dateTime === undefined) {\n        return 'All day'\n      }\n      var sTime = new Date(this.item.start.dateTime)\n      var eTime = new Date(this.item.end.dateTime)\n      return (\n        (sTime.getHours() < 10 ? '0' + sTime.getHours() : sTime.getHours()) +\n        ':' +\n        (sTime.getMinutes() < 10 ? '0' + sTime.getMinutes() : sTime.getMinutes()) +\n        ' - ' +\n        (eTime.getHours() < 10 ? '0' + eTime.getHours() : eTime.getHours()) +\n        ':' +\n        (eTime.getMinutes() < 10 ? '0' + eTime.getMinutes() : eTime.getMinutes())\n      )\n    }\n  },\n  mounted () {\n    if (this.item.colorId !== undefined) {\n      var card = this.$refs.card\n      card.classList.add('levander')\n    }\n  }\n}\n</script>\n\n<style scoped>\nli {\n  list-style: none;\n  display: block;\n  background-color: var(--background);\n  border-radius: 6px;\n  overflow: auto;\n  box-sizing: border-box;\n  margin: 1.2rem auto;\n  max-width: 512px;\n  padding: 0.8rem 1rem;\n  text-align: left;\n  -webkit-box-shadow: var(--dropshadow);\n  -moz-box-shadow: var(--dropshadow);\n  box-shadow: var(--dropshadow);\n  transition-duration: 0.6s;\n}\nli:hover {\n  background-color: var(--active);\n  cursor: pointer;\n}\n.ltr {\n  display: flex;\n  justify-content: space-between;\n}\nlabel {\n  cursor: text;\n  font-weight: 600;\n  font-size: 1.4rem;\n}\n.tags {\n  padding: 0.6rem 0;\n}\np,\n.tags {\n  margin: 0;\n}\n.levander {\n  border: 1px solid lavender !important;\n}\n</style>\n"]}]}